{
  "$schema": "https://schema.tauri.app/config/2",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "fuelcell_planeform",
  "mainBinaryName": "fuelcell_planeform",
  "version": "0.1.1",
  "identifier": "com.fuelcell-planeform.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "global-capability",
          "description": "My application capability used for all windows",
          "windows": ["*"],
          "permissions": [
            "core:window:default",
            "core:window:allow-start-dragging",
            "core:window:allow-minimize",
            "core:window:allow-close",
            "core:window:allow-toggle-maximize",
            "dialog:default",
            "dialog:allow-open"
          ]
        }
      ]
    },
    "windows": [
      {
        "title": " BAE Plane Configurator",
        "width": 1540,
        "height": 900,
        "resizable": true,
        "decorations": true,
        "transparent": true,
        "shadow": true,
        "useHttpsScheme": true
      }
    ]
  }
}
